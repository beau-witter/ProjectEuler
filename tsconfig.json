{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "allowJs": true,
    "target": "es6"
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
